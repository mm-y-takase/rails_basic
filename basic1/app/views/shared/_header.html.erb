<%= image_tag("icon.png", alt:"title") %>
<div class="menubar">
  <%= menu_link_to "top", root_path %>
  <ul>
    <li><%= menu_link_to "news", :articles %></li>
    <li><%= menu_link_to "blog", "#" %></li>
    <% if current_member %>
    <li><%= menu_link_to "account", :members %></li>
    <li><%= menu_link_to "admins", "#" %></li>
    <% end %>
  </ul>
</div>
<div>
  <%= render "shared/sidebar" %>
</div>
<div id="article_expired_area">
  <input type="checkbox" id="article_no_expiration">
  <div id="article_expired_at">aaaaaaaaaaaaaaaaaaaa</div>
</div>
<% if current_member %>
<p><%= linkto current_member.name + "さん" , :account  %><%= link_to "logout", :session, method: :delete, data: {confirm: "if your logout ?"} %></p>
<% end %>